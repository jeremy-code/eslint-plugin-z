{
  "name": "eslint-plugin-z",
  "version": "0.0.4",
  "description": "ESLint rules for Zod",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "zod"
  ],
  "homepage": "https://github.com/jeremy-code/eslint-plugin-z#readme",
  "bugs": "https://github.com/jeremy-code/eslint-plugin-z/issues",
  "license": "MIT",
  "author": "Jeremy Nguyen <hi@jeremy.ng> (https://jeremy.ng)",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremy-code/eslint-plugin-z.git"
  },
  "scripts": {
    "prepare": "husky",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.23.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.12",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/js": "^9.19.0",
    "@jest/globals": "^29.7.0",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@types/node": "^22.13.1",
    "@typescript-eslint/rule-tester": "^8.23.0",
    "@typescript-eslint/typescript-estree": "^8.23.0",
    "dedent": "^1.5.3",
    "eslint": "^9.19.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0",
    "zod": "^3.0.0"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "packageManager": "pnpm@10.2.0+sha512.0d27364e0139c6aadeed65ada153135e0ca96c8da42123bd50047f961339dc7a758fc2e944b428f52be570d1bd3372455c1c65fa2e7aa0bfbf931190f9552001"
}
