{
  "name": "eslint-plugin-z",
  "version": "0.2.0",
  "description": "ESLint rules for Zod",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "zod"
  ],
  "homepage": "https://github.com/jeremy-code/eslint-plugin-z#readme",
  "bugs": "https://github.com/jeremy-code/eslint-plugin-z/issues",
  "license": "MIT",
  "author": "Jeremy Nguyen <hi@jeremy.ng> (https://jeremy.ng)",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremy-code/eslint-plugin-z.git"
  },
  "scripts": {
    "prepare": "husky",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "check-types": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "changeset": "changeset"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "pnpm run lint",
      "pnpm run format:check"
    ],
    "*.{json,jsonc,md,yaml,yml}": "pnpm run format:check"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.33.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.5.0",
    "@eslint/js": "^9.27.0",
    "@jest/globals": "30.0.0-beta.3",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "@types/node": "^22.15.24",
    "@typescript-eslint/rule-tester": "^8.33.0",
    "@typescript-eslint/typescript-estree": "^8.33.0",
    "dedent": "^1.6.0",
    "eslint": "^9.27.0",
    "eslint-import-resolver-typescript": "^4.4.1",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-plugin-import-x": "^4.13.3",
    "eslint-plugin-jest": "^28.11.1",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-config": "30.0.0-beta.3",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "peerDependencies": {
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^8.57.0 || ^9.0.0",
    "typescript": ">=4.8.4 <5.8.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@typescript-eslint/parser": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
